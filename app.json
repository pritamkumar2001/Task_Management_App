{
  "expo": {
    "name": "Atomwalk Task Management",
    "slug": "atomwalk-task-management",
    "version": "1.0.3",
    "permissions": ["CAMERA"],
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icons.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icons.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.atomwalk.atomwalkprojectmanagement"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icons.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icons.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "14d3cafa-f99c-478a-87bd-4cce25dabf75"
      }
    },
    "owner": "atomwalk"
  }
}